turn (\n\t\t<>\n\t\t<section className='hero'>\n\t\t\t<h1>Lovely Lists</h1>\n\t\t\t<h4>Browse book lists curated by yours truly.</h4>\n\t\t</section>\n\t\t<section className='lists'>\n\t\t\t{listData.map((list) => {\n\t\t\t\tconst {title, subtitle} = list;\n\t\t\t\treturn (\n\t\t\t\t\t<article className='list'>\n\t\t\t\t\t\t<div className='list-info'>\n\t\t\t\t\t\t\t<h3>{title}</h3>\n\t\t\t\t\t\t\t<p>{subtitle}</p>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<button className='btn' type='button' onClick={(e) => handleClick(e)}>View List</button>\n\t\t\t\t\t</article>\n\t\t\t\t\t)\n\t\t\t})}\n\t\t</section>\n\t\t</>\n\n\t)\n}\n\nexport default Lists;","/Users/jonathanwyatt/Documents/Leslie/Code/react-projects/rom_recs/frontend/src/10-Rom-Recs/shelves.js",["75","76"],"import React, { useState, useEffect } from 'react';\nimport { FaTrashAlt } from 'react-icons/fa';\n\n\tconst getLocalStorage = () => {\n\t\tlet bookShelves = localStorage.getItem('bookShelves');\n\t\tif (bookShelves) {\n\t\t\treturn (bookShelves = JSON.parse(bookShelves));\n\t\t} else {\n\t\t\treturn [];\n\t\t}\n\t};\n\nconst Shelves = () => {\n\tconst [bookShelves, setBookShelves] = useState(getLocalStorage());\n\n\tconst handleClick = (e, id) => {\n\t\te.preventDefault();\n\t\tlocalStorage.setItem('bookShelves', JSON.stringify(bookShelves.filter((book) => book.id !== id)));\n\t\tsetBookShelves(getLocalStorage());\n\t}\n\n\tif (bookShelves.length < 1 || bookShelves === undefined) {\n\t\treturn (\n\t\t\t<>\n\t\t\t<section className='hero'>\n\t\t\t\t<h1>Shelves</h1>\n\t\t\t\t<h4>Good books deserve to be saved.</h4>\n\t\t\t</section>\n\t\t\t<section className='shelves'>\n\t\t\t\t<img src={process.env.PUBLIC_URL + `/images/shelves.svg`} className='shelves-img' alt='girl reading' />\n\t\t\t\t<p>Your shelves are empty! To save books to your shelves, click the save button when you see a book on the <a href=\"/\">homepage</a>.</p>\n\t\t\t</section>\n\t\t\t</>\n\t\t\t)\n\t} else {\n\t\t\treturn (\n\t\t\t\t<>\n\t\t\t\t<section className='hero'>\n\t\t\t\t\t<h1>Shelves</h1>\n\t\t\t\t\t<h4>Good books deserve to be saved.</h4>\n\t\t\t\t</section>\n\t\t\t\t<section className='shelves'>\n\t\t\t\t\t{bookShelves.map((book) => {\n\t\t\t\t\t\tconst {title, author, img, link, id} = book;\n\t\t\t\t\t\treturn (\n\t\t\t\t\t\t\t<article className='list' key={id}>\n\t\t\t\t\t\t\t\t<div className='list-info'>\n\t\t\t\t\t\t\t\t\t<a href={link}><h3>{title}</h3></a>\n\t\t\t\t\t\t\t\t\t<h4>{author}</h4>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<button className='btn' type='button' onClick={(e) => handleClick(e, id)}><FaTrashAlt/></button>\n\t\t\t\t\t\t\t</article>\n\t\t\t\t\t\t\t)\n\t\t\t\t\t})}\n\t\t\t\t</section>\n\t\t\t\t</>\n\t\t\t)\n\t}\n}\n\nexport default Shelves;","/Users/jonathanwyatt/Documents/Leslie/Code/react-projects/rom_recs/frontend/src/10-Rom-Recs/home.js",[],"/Users/jonathanwyatt/Documents/Leslie/Code/react-projects/rom_recs/frontend/src/10-Rom-Recs/list-data.js",["77"],"import React from 'react';\n\nexport const listData = [\n{\n\ttitle: 'Steamy retellings',\n\tsubtitle: 'Classic stories retold with romantic twists'\n},\n{\n\ttitle: 'Dancing rakes',\n\tsubtitle: 'Regency romance filled with lords and ladies attending balls'\n},\n{\n\ttitle: 'Witches, vampires, and ghosts oh my...',\n\tsubtitle: 'Many things go bump in the night'\n},\n{\n\ttitle: 'Ladies who save the day',\n\tsubtitle: 'Women who save the day and happen to fall in love too'\n},\n{\n\ttitle: 'Prophecies, quests, and true love',\n\tsubtitle: 'Journey with hero(ine)s as they find love and adventure'\n}\n];","/Users/jonathanwyatt/Documents/Leslie/Code/react-projects/rom_recs/frontend/src/10-Rom-Recs/navlinks.js",[],"/Users/jonathanwyatt/Documents/Leslie/Code/react-projects/rom_recs/frontend/src/10-Rom-Recs/filters.js",["78"],"import React, { useState }from 'react';\nimport { FaSlidersH, FaRegWindowClose} from \"react-icons/fa\";\nimport MobileFilters from './mobile-filters';\nimport Categories from './categories';\n\nconst Filters = ({ filteredBooks, setFilteredBooks, allBooks, setAllBooks, showFilters, mobile, setShowFilters, bookRec, setBookRec }) => {\n\n\tconst [checked, setChecked] = useState('All')\n\tconst handleSelection = (category) => {\n\t\tlet filtered = allBooks;\n\t\tif (category === 'All') {\n\t\t\tconsole.log('category is all');\n\t\t} else {\n\t\t\tfiltered = allBooks.filter((book) => book.category === category);\n\t\t}\n\t\tlet random = Math.floor(Math.random() * filtered.length);\n\t\tsetBookRec(random);\n\t\tsetFilteredBooks(filtered);\n\t\tsetChecked(category)\n\t}\n\n\tif (showFilters && mobile) {\n\t\treturn (\n\t\t\t<div className='popup-overlay'>\t\n\t\t\t<div className='popup-categories'>\n\t\t\t\t<div className='popup-title'>\n\t\t\t\t\t<h3>Refine Results </h3>\n\t\t\t\t\t<h3><FaRegWindowClose style={{verticalAlign: 'middle', cursor: 'pointer'}} onClick={() => {setShowFilters(false)}}/></h3>\n\t\t\t\t</div>\n\t\t\t\t<form action=\"\" className='popup-form'>\n\t\t\t\t\t<MobileFilters checked={checked} handleSelection={handleSelection} showFilters={showFilters} setShowFilters={setShowFilters}></MobileFilters>\n\t\t\t</form>\n\t\t\t</div>\n\t\t</div>\n\t\t\t)\n\t}\n\tif (mobile || bookRec === -1) {\n\t\treturn null;\n\t}\n\treturn (\n\t\t<div className='categories'>\n\t\t\t<h4>Refine results  <FaSlidersH style={{verticalAlign: 'middle'}} /></h4>\n\t\t\t<form>\n\t\t\t\t<MobileFilters checked={checked} handleSelection={handleSelection} showFilters={showFilters} setShowFilters={setShowFilters}></MobileFilters>\n\t\t\t</form>\n\t\t</div>\n\t)\n};\n\nexport default Filters;","/Users/jonathanwyatt/Documents/Leslie/Code/react-projects/rom_recs/frontend/src/10-Rom-Recs/eros.js",[],"/Users/jonathanwyatt/Documents/Leslie/Code/react-projects/rom_recs/frontend/src/10-Rom-Recs/rec.js",["79","80"],"/Users/jonathanwyatt/Documents/Leslie/Code/react-projects/rom_recs/frontend/src/10-Rom-Recs/books.js",[],["81","82"],"/Users/jonathanwyatt/Documents/Leslie/Code/react-projects/rom_r